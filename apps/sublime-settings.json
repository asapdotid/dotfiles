{
    "ignored_packages":
    [
        "Markdown",
        "Vintage",
    ],
    "font_face": "Fira Code",
    "font_size": 13,
    "font_options": [
        "subpixel_antialias"
    ],
    "line_padding_top": 10,
    "line_padding_bottom": 10,
    "word_wrap": true,
    "highlight_line": true,
    "translate_tabs_to_spaces": true,
    "highlight_modified_tabs": true,
    "theme": "Adaptive.sublime-theme",
    "color_scheme": "Mariana.sublime-color-scheme",
}
